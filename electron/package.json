{
  "name": "earthcal",
  "version": "1.1.7",
  "description": "Sync your moments with Earth's cycles",
  "main": "main.js",
  "scripts": {
    "start": "electron . --no-sandbox",
    "prepare-public": "./prepare-public.sh",
    "package": "npm run prepare-public && electron-packager . earthcal --platform=linux --arch=x64 --out=./ --overwrite",
    "snap": "npm run package && electron-installer-snap --src earthcal-linux-x64/ --dest snap/",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/russs95/earthcalendar.git"
  },
  "keywords": [
    "calendar",
    "cycles",
    "events"
  ],
  "author": "Russell Maier",
  "license": "MIT",
  "devDependencies": {
    "electron": "^26.6.10",
    "electron-installer-snap": "^5.2.0",
    "electron-packager": "^17.1.2",
    "jest": "^30.0.5"
  }
}
